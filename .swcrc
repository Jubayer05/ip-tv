{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "ecmascript",
      "jsx": true,
      "dynamicImport": true,
      "decorators": false
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2020",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "minify": false,
  "env": {
    "targets": {
      "chrome": "80",
      "firefox": "78",
      "safari": "13.1",
      "edge": "80"
    },
    "mode": "entry",
    "coreJs": "3"
  }
}
